{
  "version": "0.2.0",
  "inputs": [
    {
      "id": "password",
      "type": "promptString",
      "description": "Password for the Unifi Controller",
      "default": "password"
    }
  ],
  "configurations": [

    {
      "name": "Python: Module",
      "type": "python",
      "request": "launch",
      "module": "aiounifi",
      "justMyCode": true,
      "args": [
        "unifi.kempton.app",
        "viewinfo",
        "${input:password}",
        "--port", "443",
        "--debug",
        ]
    }
  ]
}
